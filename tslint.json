{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "no-empty": false,
    "no-submodule-imports": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-first"
      }
    ]
  }
}
